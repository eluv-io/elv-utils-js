{
  "default-clear-download": [
    {
      "op": "replace",
      "path": "/drm_optional",
      "value": true
    },
    {
      "op": "replace",
      "path": "/playout/playout_formats",
      "value": {
        "dash-clear": {
          "drm": null,
          "protocol": {
            "min_buffer_length": 2,
            "type": "ProtoDash"
          }
        },
        "hls-clear": {
          "drm": null,
          "protocol": {
            "type": "ProtoHls"
          }
        }
      }
    }
  ]
}
